{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "eofline": true,
    "semicolon": [
      true,
      "always"
    ],
    "member-access": false,
    "trailing-comma": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "no-console": false,
    "arrow-parens": false,
    "no-angle-bracket-type-assertion": false,
    "whitespace": [
      true,
      "check-branch",
      "check-operator"
    ],
    "indent": [
      true,
      "spaces",
      2
    ]
  },
  "rulesDirectory": []
}